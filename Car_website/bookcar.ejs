<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="Awaiken Theme">
    <!-- Page Title -->
    <title>The Car Rent Shop</title>
    <!-- Google Fonts css-->
    <link
        href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700,800,900%7CPlayfair+Display:400,700,900"
        rel="stylesheet">
    <!-- Bootstrap css -->
    <link href="css/bootstrap.min.css" rel="stylesheet" media="screen">
    <!-- Font Awesome icon css-->
    <link href="css/font-awesome.min.css" rel="stylesheet" media="screen">
    <link href="css/pe-icon-7-stroke.css" rel="stylesheet" media="screen">
    <link href="css/flaticon.css" rel="stylesheet" media="screen">
    <!-- Carousel css -->
    <link rel="stylesheet" href="css/owl.carousel.css">
    <!-- Magnific Popup CSS -->
    <link href="css/magnific-popup.css" rel="stylesheet">
    <!-- Slick nav css -->
    <link rel="stylesheet" href="css/slicknav.css">
    <!-- Grid Gallery css -->
    <link rel="stylesheet" data-template-style="true" type="text/css" href="css/CSTiles-1.1.0.css">
    <!-- Main custom css -->
    <link href="css/custom.css" rel="stylesheet" media="screen">

</head>

<body class="bg' >
    <!-- Preloader starts -->
    <div class="preloader">
        <div class="browser-screen-loading-content">
            <div class="loading-dots dark-gray">
                <img src="images/puff.svg" alt="Preloader" />
            </div>
        </div>
    </div>
    <!-- Preloader Ends -->

    <!-- Header Section Starts-->
    <header>
        <nav id="main-nav" class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    
                    <!-- Responsive Menu button starts -->
                    <div class="navbar-toggle">
                    </div>
                    <!-- Responsive Menu button Ends -->
                </div>

                <div id="responsive-menu"></div>

                <!-- Navigation starts -->
                <div class="navbar-collapse collapse" id="navigation">
                    <ul class="nav navbar-nav navbar-right main-navigation" id="main-menu">
                        <li>
                            <!-- <a href="bookcar">Book Car</a> -->
                        </li>
                        <li>
                            <a href="viewbookings">View Bookings</a>
                        </li>
                        <li>
                            <!-- <a href="profile">Profile</a> -->
                        </li>
                        <li>
                            <a href="logoutme">Logout</a>
                        </li>
                    </ul>
                </div>
                <!-- Navigation Ends -->
            </div>
        </nav>
    </header>
    <!-- Header Section Ends-->

    <!-- Banner Section Starts -->
    <section class="banner" id="home">
        <div class="container">
        </div>
    </section>
    <!-- Banner Section Ends -->





    <!-- Contact us section starts -->
    <section class="contactus" id="contact">
        <div class="google-map">
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="contact-box">
                        <div class="row">


                            <div class="col-md-12">
                                <div id="content" style="width: 1000px;">
                                    <%
                                if(alert=="success")
                                {
                                    %>
                                    <span style="text-align:center;color:green;font-size:20px">Table Booked
                                        Successfully</span>
                                    <%
                                }
                              %>
                                    <%
                                if(alert=="fail")
                                {
                                    %>
                                    <span style="text-align:center;color:rgb(221, 14, 14);font-size:20px">Table
                                        AlreTaken</span>
                                    <%
                                }
                                %>
                                    <h2 style="text-align: center;">Booking A Car</h2>
                                    <table align="center">
                                        <form action="addbooking" method="POST">
                                            <tr>
                                                <div class="gallery">
                                                      <img src="<%=car[0].carimage%>" alt="honda_civic" width="800" height="400">
                                                     <div class="description"><%=car[0].type%>: <%= car[0].model%></div>
                                                     <input hidden name='carid' value='<%=car[0].carid%>'/>
                                                  </div>
                                                <!-- <td> Car type: </td>
                                                <td>
                                                    <select class="form-control" name="cartype" required>
                                                        <option value="">Select</option>
                                                        <option value="Economy">Economy</option>
                                                        <option value="SUV">SUV</option>
                                                        <option value="Luxury">Luxury</option>
                                                    </select>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td> Car Model: </td>
                                                <td>
                                                    <select class="form-control" name="carmodel" required>
                                                        <option value="">Select Car</option>
                                                        <option value="Economy">Economy</option>
                                                        <option value="SUV">SUV</option>
                                                        <option value="Luxury">Luxury</option>
                                                    </select>
                                                </td> -->
                                            </tr>



                                            <tr>
                                                <td>
                                                    Select Pickup : </td>
                                                <td>
                                                    <input class="form-control" type="date" name="pdate" required />
                                                </td>
                                                <td>

                                                    <select class="form-control" name="ptime" required>
                                                        <option value="">Select Pickup Time</option>
                                                        <option value="10">10</option>
                                                        <option value="11">11</option>
                                                        <option value="12">12</option>
                                                        <option value="13">13</option>
                                                        <option value="14">14</option>
                                                        <option value="15">15</option>
                                                        <option value="16">16</option>
                                                        <option value="17">17</option>
                                                        <option value="18">18</option>
                                                        <option value="19">19</option>
                                                    </select>
                                                </td>
                                                <tr>
                                                <td>
                                                    Select Delivery:</td>
                                                <td>
                                                    <input class="form-control" type="date" name="ddate" required />
                                                </td>
                                                <td>
                                                    <select class="form-control" name="dtime" required>
                                                        <option value="">Select Delivery Time</option>
                                                        <option value="10">10</option>
                                                        <option value="11">11</option>
                                                        <option value="12">12</option>
                                                        <option value="13">13</option>
                                                        <option value="14">14</option>
                                                        <option value="15">15</option>
                                                        <option value="16">16</option>
                                                        <option value="17">17</option>
                                                        <option value="18">18</option>
                                                        <option value="19">19</option>
                                                    </select>
                                                </td>
                                               </tr> 
                                            </tr>
                                            <tr>
                                                <td colspan=2 align="center"><input class="btn-custom" type="submit" />
                                                </td>
                                            </tr>
                                        </form>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Contact us section ends -->

    <!-- Footer section starts -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                 </div>
            </div>
        </div>
    </footer>
    <!-- Footer section ends -->

    <!-- Jquery Library File -->
    <script src="js/jquery-1.12.4.min.js"></script>
    <!-- SmoothScroll -->
    <script src="js/SmoothScroll.js"></script>
    <!-- Bootstrap js file -->
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.counterup.min.js"></script>
    <!-- Slick Nav js file -->
    <script src="js/jquery.slicknav.js"></script>
    <!-- Magnific Popup js file -->
    <script src="js/jquery.magnific-popup.js"></script>
    <!-- Owl Carousel js file -->
    <script src="js/owl.carousel.js"></script>
    <!-- CS Tiles Gallery js file -->
    <script src="js/jquery.CSTiles-1.1.0.min.js"></script>
    <!-- Main Custom js file -->
    <script src="js/function.js"></script>
</body>

</html>